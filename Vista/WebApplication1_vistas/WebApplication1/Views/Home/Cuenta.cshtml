                        <div class="form-group">
                            <input type="text" class="form-control form-float" placeholder="Municipio / Delegación" required id="municipio">
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-12">
                        <p class="my-2">Colonia: *</p>
                        <div class="form-group">
                            <input type="text" class="form-control form-float" placeholder="Colonia" id="colonia">
                        </div>
                    </div>
