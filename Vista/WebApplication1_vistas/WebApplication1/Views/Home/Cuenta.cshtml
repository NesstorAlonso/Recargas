            <div class="block-header">
                <h2><strong>Alertas</strong> notificación *</h2>
            </div>
            <div class="card">
                <div class="body">
                    <div id="Notificaciones" class="row clearfix">
                        <div class="col-lg-6 col-md-12">
                            <div class="form-group form-float">
                                <div class="checkbox">
                                    <input id="checkbox_cc" type="checkbox" class="form-control" name="Notificaciones[]" value="NotificacionCompraPorCorreo">
                                    <label for="checkbox_cc">Notificación de compra por correo</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-12">
                            <div class="form-group form-float">
                                <div class="checkbox">
                                    <input id="checkbox_cs" type="checkbox" class="form-control" name="Notificaciones[]" value="NotificacionCompraPorSMS">
                                    <label for="checkbox_cs">Notificación de compra por SMS</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-12">
                            <div class="form-group form-float">
                                <div class="checkbox">
                                    <input id="checkbox_ms" type="checkbox" class="form-control" name="Notificaciones[]" value="AlertaSaldoMinimoPorSMS">
                                    <label for="checkbox_ms">Alerta saldo mínimo por SMS</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-12">
                            <div class="input-group form-float">
                                <span class="input-group-addon"><i class="zmdi zmdi-money"></i></span>
                                <input id="MontoAlertas" type="number" class="form-control" name="calertas" required placeholder="Si no requiere monto dejar en 0" value="0">
                                @*<div class="help-info">Solo valores numéricos</div>*@
                                @*<button class="btn btn-mdb-color btn-rounded btn-sm my-0 ml-sm-2" id="btnAgregarNotifica">Agregar</button>*@
                            </div>

                        </div>
                    </div>
                </div>
            </div>

