                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="overlay"></div><!-- Overlay For Sidebars -->

    <nav class="navbar">
        <div class="container">
            <ul class="nav navbar-nav">
                <li>
                    <div class="navbar-header">
                        <a href="javascript:void(0);" class="h-bars"></a>
                        <h6 class="navbar-brand" asp-controller="Home" asp-action="Main" id="Index"><img src="~/assets/images/LogoRE.png" width="35" alt="Alpino"><span class="m-l-10">RECARGACELULARES.COM.MX</span></h6>
                    </div>
                </li>
                @*<li class="search_bar">
                    <div class="input-group">
                        <input type="text" class="form-control" placeholder="Buscar...">
                    </div>
                </li>*@

                @*<li class="float-right">
